{
  "oxc.enable.oxlint": true,
  "editor.codeActionsOnSave": ["source.fixAll.oxc"],
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.exclude": {
    "coverage": true,
    "**/*.tsbuildinfo": true,
    "**/junit.xml": true
  }
}
